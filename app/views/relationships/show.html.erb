<div class="page-header">
  <h3>Relationship #<%= @relationship.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">

      <% @relationships.each do |relationship| %>

      <dt>Relationship type</dt>
      <dd><%= relationship.relationship_type %></dd>

      <dt>Other member</dt>
      <dd><%= relationship.other_member_id %></dd>

      <dt>Member</dt>
      <dd><%= relationship.member.name %></dd>

      <p></p>

          <% end %>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/relationships" class="btn btn-primary">
        Back
      </a>
      <a href="/relationships/<%= @relationship.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_relationship/<%= @relationship.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
